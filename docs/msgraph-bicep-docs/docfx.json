{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "conceptual",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "reference/beta",
        "dest": "./reference",
        "version": "graph-bicep-beta",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "reference/v1.0",
        "dest": "./reference",
        "version": "graph-bicep-1.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/graph/templates/breadcrumb/toc.json",
      "feedback_system": "Standard",
      "permissioned-type": "public",
      "titleSuffix": "Microsoft Graph Bicep",
      "uhfHeaderId": "MSDocsHeader-MSGraphBicep",
      "ms.service": "microsoft-graph-bicep",
      "ms.subservice": "non-product-specific",
      "manager": "celestedg",
      "ms.reviewer": "ombongifaith"
    },
    "fileMetadata": {
      "author":
      {
        "reference/**/*.md": "dkershaw10"
      },
      "ms.author":
      {
        "reference/**/*.md": "dkershaw"
      }
    },
    "template": []
  }
}